<div class="questionnaires_<%= action_name %> ">
  <h1>
      Surveys Index
  </h1>
  <hr>
  <% if current_user.admin? %>
  <%= link_to "New Survey", new_survey %>
  <% else %>
  <%#= will_paginate %>

  <ul>
    <% @surveys.each do |survey| %>
      <li>
        <%= link_to survey.name, edit_survey(survey) %>
      </li>
    <% end -%>
    <% end %>
  </ul>
   <%#= will_paginate %>
</div>
